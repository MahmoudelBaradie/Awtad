<odoo>
    <record id="view_maj_state_log_tree" model="ir.ui.view">
        <field name="name">maj.state.log.tree</field>
        <field name="model">maj.state.log</field>
        <field name="arch" type="xml">
            <list create="0" edit="0" delete="0">
                <field name="changed_on"/>
                <field name="model_name"/>
                <field name="record_display_name"/>
                <field name="previous_state"/>
                <field name="new_state"/>
                <field name="changed_by"/>
            </list>
        </field>
    </record>

    <record id="action_maj_state_log" model="ir.actions.act_window">
        <field name="name">State Audit Log</field>
        <field name="res_model">maj.state.log</field>
        <field name="view_mode">list</field>
    </record>
</odoo>
