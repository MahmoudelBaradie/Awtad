<odoo>
    <record id="group_real_estate_accountant" model="res.groups">
        <field name="name">Real Estate Accountant</field>
    </record>

    <record id="group_real_estate_project_manager" model="res.groups">
        <field name="name">Real Estate Project Manager</field>
    </record>

    <record id="group_real_estate_sales_manager" model="res.groups">
        <field name="name">Real Estate Sales Manager</field>
    </record>

    <record id="group_real_estate_general_manager" model="res.groups">
        <field name="name">Real Estate General Manager</field>
        <field name="implied_ids" eval="[(4, ref('group_real_estate_accountant')), (4, ref('group_real_estate_project_manager')), (4, ref('group_real_estate_sales_manager'))]"/>
    </record>
</odoo>
