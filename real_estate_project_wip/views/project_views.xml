<odoo>
<record id="view_real_estate_project_tree" model="ir.ui.view"><field name="name">real.estate.project.tree</field><field name="model">real.estate.project</field><field name="arch" type="xml"><list><field name="name"/><field name="land_id"/><field name="estimated_budget"/><field name="actual_cost"/><field name="variance"/><field name="state"/></list></field></record>
<record id="view_real_estate_project_form" model="ir.ui.view"><field name="name">real.estate.project.form</field><field name="model">real.estate.project</field><field name="arch" type="xml"><form><header><button name="action_start" type="object" string="Start" invisible="state != 'draft'"/><button name="action_close_project" type="object" string="Close Project" invisible="state != 'running'" class="btn-primary"/><field name="state" widget="statusbar" statusbar_visible="draft,running,closed"/></header><sheet><group><field name="name"/><field name="land_id"/><field name="estimated_budget"/><field name="actual_cost" readonly="1"/><field name="variance" readonly="1"/><field name="conversion_type"/></group><notebook><page string="Actual Costs"><field name="cost_line_ids"><list editable="bottom"><field name="date"/><field name="category"/><field name="description"/><field name="vendor_id"/><field name="amount"/></list></field></page></notebook></sheet></form></field></record>
<record id="action_real_estate_project" model="ir.actions.act_window"><field name="name">Projects (WIP)</field><field name="res_model">real.estate.project</field><field name="view_mode">list,form</field></record>
<menuitem id="menu_real_estate_project" name="Projects" parent="real_estate_core.menu_real_estate_root" action="action_real_estate_project"/>
</odoo>